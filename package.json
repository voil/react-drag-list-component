{
  "name": "react-drag-list-component",
  "version": "1.0.9",
  "description": "simple react component drag list",
  "main": "index.js",
  "keywords": ["react", "drag", "list", "component"],
  "author": "Przemys≈Çaw Drzewicki <przemyslaw.drzewicki@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voil/react-drag-list-component.git"
  },
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "build": "babel ./src/lib/index.js > dist/react-drag-list-component.js --no-comments && babel ./src/lib/utils.js > dist/utils.js --no-comments && babel ./src/lib/template.js > dist/template.js --no-comments && babel ./src/export.js > index.js --no-comments",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "testRegex": "\\.spec\\.js$",
    "setupFiles": [
      "./__tests__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      ".js": "./node_modules/babel-jest"
    }
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --config webpack/webpack.bulid.js --watch --progress",
      "env": {
        "ENV": "development",
        "APP_TITLE": "React component - drag list",
        "HOST_PORT": 3101
      }
    }
  },
  "devDependencies": {
    "babel-core": "6.16.0",
    "babel-loader": "6.2.7",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "concurrently": "^3.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.25.0",
    "file-loader": "0.8.5",
    "html-loader": "^0.4.4",
    "imports-loader": "^0.6.5",
    "node-sass": "3.12.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "3.2.3",
    "sass-resources-loader": "^1.2.0",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "2.2.0",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "clean-webpack-plugin": "^0.1.13",
    "compression-webpack-plugin": "^0.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "es6-enum": "^1.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "3.2.0",
    "react-test-renderer": "16.2.0",
    "webpack": "2.2.0",
    "yargs": "^10.0.3"
  }
}
